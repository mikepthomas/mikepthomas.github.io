{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "template": "[[workspace]]/.frontmatter/templates/default.md",
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Created date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last Modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.seoContentLengh": 2400,
  "frontMatter.panel.actions.disabled": ["optimizeSlug"],
  "frontMatter.framework.id": "other",
  "frontMatter.framework.startCommand": "npm start",
  "frontMatter.git.enabled": true,
  "frontMatter.content.publicFolder": "",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.website.host": "https://mikethomas.info",
  "frontMatter.content.pageFolders": [
    {
      "title": "projects",
      "path": "[[workspace]]/src/data/markdown"
    }
  ],
  "frontMatter.data.files": [
    {
      "id": "experience",
      "title": "Experience",
      "file": "[[workspace]]/src/data/Experience.json",
      "fileType": "json",
      "labelField": "title",
      "singleEntry": false,
      "schema": {
        "title": "ExperienceItem",
        "type": "object",
        "required": ["title", "startDate", "city", "location", "icon"],
        "properties": {
          "title": {
            "type": "string",
            "title": "Title"
          },
          "startDate": {
            "type": "string",
            "title": "Start Date"
          },
          "endDate": {
            "type": "string",
            "title": "End Date"
          },
          "city": {
            "type": "string",
            "title": "City"
          },
          "location": {
            "type": "string",
            "title": "Location"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "description": {
            "type": "string",
            "multiline": true,
            "title": "Description"
          },
          "color": {
            "type": "string",
            "title": "Color"
          },
          "icon": {
            "type": "string",
            "title": "Icon"
          },
          "inverted": {
            "type": "boolean",
            "title": "Inverted"
          }
        }
      }
    },
    {
      "id": "social",
      "title": "Social",
      "file": "[[workspace]]/src/data/Social.json",
      "fileType": "json",
      "labelField": "name",
      "singleEntry": false,
      "schema": {
        "title": "SocialItem",
        "type": "object",
        "required": ["name", "icon", "url", "user", "showInNav"],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "icon": {
            "type": "string",
            "title": "Icon Name"
          },
          "url": {
            "type": "string",
            "title": "url"
          },
          "user": {
            "type": "string",
            "title": "Username"
          },
          "project": {
            "type": "string",
            "title": "Project"
          },
          "showInNav": {
            "type": "boolean",
            "title": "Show in navigation"
          }
        }
      }
    }
  ]
}
